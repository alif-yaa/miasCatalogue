               <idno type="RG">269</idno>
               <idno type="Alt. RG"/>
               <idno type="RECORDID">27</idno>
               <p/>
               <title xml:lang="ara-Latn-x-lc" type="uniform">Ijāza li-l-malik al-Muẓaffar</title>
               <title xml:lang="ara" type="uniform-ara">إجازة للملك المظفر</title>
               <title xml:lang="ara-Latn-x-lc" type="abbreviated">Ijaza</title>
