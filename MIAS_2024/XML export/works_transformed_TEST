            <bibl xml:id="">
               <idno type="RG">98</idno>
               <idno type="Alt. RG"/>
               <idno type="RECORDID">8</idno>
               <p/>
               <title xml:lang="ara-Latn-x-lc" type="uniform">Daʿwat al-asmāʾ al-ḥusnā</title>
