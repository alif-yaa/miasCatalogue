               <p/>
               <title xml:lang="ara-Latn-x-lc" type="uniform">Tahdhīb al-akhlāq</title>
               <title xml:lang="ara" type="uniform-ara">تهذيب الأخلاق</title>
               <title xml:lang="ara-Latn-x-lc" type="abbreviated">Tahdhib akhlaq</title>
               <title xml:lang="ara-Latn-x-lc" type="alternate">Insert Alternate Title(s)</title>
               <p/>
               <author key="person_0000">Yahyā b. Adī (d.974)</author>
               <persName role="dub" key="person_0001">Ibn al-ʿArabī</persName>
               <p/>
               <date type="ms" calendar="Hijri-qamari"/>
               <date type="composition" calendar="Hijri-qamari">591</date>
               <placeName type="composition"/>
               <note type="composition"/>
               <date when="YYYY" cert="high" calendar="Hijri-qamari">Insert Exact Date of Composition</date>
